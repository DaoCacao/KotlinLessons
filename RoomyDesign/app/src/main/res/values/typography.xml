<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="RoomyText" parent="">
        <item name="android:textSize">14sp</item>
        <item name="android:layout_weight">500</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:height">16sp</item>
        <item name="android:lineSpacingExtra">1.25sp</item>
        <item name="android:typeface">monospace</item>
    </style>

    <style name="RoomyText.FilledButton" parent="RoomyText">
        <item name="android:textColor">@color/design_default_color_on_primary</item>
    </style>

    <style name="RoomyText.AppBarText" parent="RoomyText">
        <item name="android:textSize">20sp</item>
        <item name="android:layout_weight">500</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:height">24sp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:typeface">monospace</item>
    </style>
</resources>